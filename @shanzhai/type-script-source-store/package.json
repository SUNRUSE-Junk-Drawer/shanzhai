{
  "name": "@shanzhai/type-script-source-store",
  "description": "A Shanzhai store for TypeScript source files.",
  "version": "0.0.8",
  "engines": {
    "node": ">=17.6.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.*",
    "!test-data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/shanzhai"
  },
  "license": "MIT",
  "dependencies": {
    "@shanzhai/ephemeral-keyed-store": "0.0.2"
  },
  "types": "index.d.ts"
}
