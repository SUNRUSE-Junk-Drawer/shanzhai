{
  "name": "@shanzhai/render-pug-plugin",
  "description": "A Shanzhai plugin which renders parsed Pug, storing the results in the HTML source store.  File extensions of \".pug\" are automatically replaced with \".html\".",
  "version": "0.0.13",
  "engines": {
    "node": ">=17.6.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.*",
    "!test-data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/shanzhai"
  },
  "license": "MIT",
  "dependencies": {
    "@shanzhai/delete-from-keyed-store-step": "0.0.5",
    "@shanzhai/keyed-store-get-all-input": "0.0.5",
    "@shanzhai/keyed-store-get-input": "0.0.5",
    "@shanzhai/keyed-store-set-output": "0.0.5",
    "@shanzhai/merge-object-input": "0.0.10",
    "@shanzhai/render-pug-step": "0.0.10"
  },
  "devDependencies": {
    "@shanzhai/interfaces": "0.0.25",
    "@shanzhai/parsed-pug-store": "0.0.8",
    "@shanzhai/html-source-store": "0.0.9",
    "@shanzhai/pug-local-store": "0.0.9"
  },
  "peerDependencies": {
    "@shanzhai/parsed-pug-store": "0.0.8",
    "@shanzhai/html-source-store": "0.0.9",
    "@shanzhai/pug-local-store": "0.0.9"
  },
  "types": "index.d.ts",
  "shanzhaiPlugin": [
    "."
  ]
}
