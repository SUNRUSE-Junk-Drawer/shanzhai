{
  "name": "@shanzhai/javascript-source-store",
  "description": "A Shanzhai store for Javascript source files.",
  "version": "0.0.13",
  "engines": {
    "node": ">=17.6.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.*",
    "!test-data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/shanzhai"
  },
  "license": "MIT",
  "dependencies": {
    "@shanzhai/ephemeral-unkeyed-store": "0.0.4"
  },
  "devDependencies": {
    "@shanzhai/interfaces": "0.0.25"
  },
  "types": "index.d.ts"
}
