{
  "name": "@shanzhai/minify-javascript-plugin",
  "description": "A Shanzhai plugin which minifies the Javascript from the source store, writing the resulting Javascript to the Pug locals store.",
  "version": "0.0.13",
  "engines": {
    "node": ">=17.6.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.*",
    "!test-data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/shanzhai"
  },
  "license": "MIT",
  "dependencies": {
    "@shanzhai/keyed-store-get-all-input": "0.0.6",
    "@shanzhai/keyed-store-set-output": "0.0.6",
    "@shanzhai/merge-object-input": "0.0.11",
    "@shanzhai/minify-javascript-step": "0.0.14",
    "@shanzhai/unkeyed-store-get-input": "0.0.6",
    "@shanzhai/wrap-in-object-output": "0.0.7"
  },
  "devDependencies": {
    "@shanzhai/interfaces": "0.0.26",
    "@shanzhai/javascript-source-store": "0.0.15",
    "@shanzhai/pug-local-store": "0.0.10",
    "@shanzhai/type-script-global-store": "0.0.16"
  },
  "peerDependencies": {
    "@shanzhai/javascript-source-store": "0.0.15",
    "@shanzhai/type-script-global-store": "0.0.16",
    "@shanzhai/pug-local-store": "0.0.10"
  },
  "types": "index.d.ts",
  "shanzhaiPlugin": [
    "."
  ]
}
