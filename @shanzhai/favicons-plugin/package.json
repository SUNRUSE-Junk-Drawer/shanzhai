  "devDependencies": {
    "@shanzhai/interfaces": "0.0.26",
    "@shanzhai/favicon-store": "0.0.3",
    "@shanzhai/favicons-options-store": "0.0.3",
    "@shanzhai/html-header-store": "0.0.6",
    "@shanzhai/zip-content-store": "0.0.7",
    "@types/favicons": "6.2.2"
  },
  "peerDependencies": {
    "@shanzhai/favicon-store": "0.0.3",
    "@shanzhai/favicons-options-store": "0.0.3",
    "@shanzhai/html-header-store": "0.0.6",
