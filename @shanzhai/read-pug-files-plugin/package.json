{
  "name": "@shanzhai/read-pug-files-plugin",
  "description": "A Shanzhai plugin which reads Pug files into the Pug source store.",
  "version": "0.0.8",
  "engines": {
    "node": ">=17.6.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.*",
    "!test-data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/shanzhai"
  },
  "license": "MIT",
  "dependencies": {
    "@shanzhai/delete-from-keyed-store-step": "0.0.2",
    "@shanzhai/keyed-store-set-output": "0.0.4",
    "@shanzhai/read-text-file-step": "0.0.9"
  },
  "devDependencies": {
    "@shanzhai/interfaces": "0.0.24",
    "@shanzhai/pug-source-store": "0.0.5"
  },
  "peerDependencies": {
    "@shanzhai/pug-source-store": "0.0.5"
  },
  "types": "index.d.ts",
  "shanzhaiPlugin": [
    "."
  ]
}
