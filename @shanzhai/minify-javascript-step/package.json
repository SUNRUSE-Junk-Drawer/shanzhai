{
  "name": "@shanzhai/minify-javascript-step",
  "description": "A Shanzhai step which minifies a Javascript file.",
  "version": "0.0.11",
  "engines": {
    "node": ">=17.6.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
