    "type": "git",
    "url": "https://github.com/jameswilddev/shanzhai"
  },
  "license": "MIT",
  "dependencies": {
    "uglify-js": "3.15.3",
    "@shanzhai/minify-step": "0.0.9"
  },
  "devDependencies": {
    "@shanzhai/interfaces": "0.0.24",
    "@types/uglify-js": "3.13.1"
  },
  "types": "index.d.ts"
}
