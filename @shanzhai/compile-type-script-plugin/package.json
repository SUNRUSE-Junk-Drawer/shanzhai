{
  "name": "@shanzhai/compile-type-script-plugin",
  "description": "A Shanzhai plugin while compiles the entire store of parsed TypeScript, storing the resulting Javascript in corresponding source store.",
  "version": "0.0.2",
  "engines": {
    "node": ">=12.13.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.*",
    "!test-data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/shanzhai"
  },
  "license": "MIT",
  "dependencies": {
    "@shanzhai/compile-type-script-step": "0.0.9",
    "@shanzhai/keyed-store-get-all-input": "0.0.1",
    "@shanzhai/unkeyed-store-get-input": "0.0.1",
    "@shanzhai/unkeyed-store-set-output": "0.0.1"
  },
  "devDependencies": {
    "@shanzhai/interfaces": "0.0.20",
    "@shanzhai/javascript-source-store": "0.0.8",
    "@shanzhai/parsed-type-script-store": "0.0.8",
    "@shanzhai/type-script-compiler-options-store": "0.0.7"
  },
  "peerDependencies": {
    "@shanzhai/type-script-compiler-options-store": "0.0.6",
    "@shanzhai/javascript-source-store": "0.0.8",
    "@shanzhai/parsed-type-script-store": "0.0.7"
  },
  "types": "index.d.ts"
}
