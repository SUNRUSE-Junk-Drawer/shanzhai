{
  "name": "@shanzhai/production-cli",
  "description": "A CLI tool which performs a one-off production build of the Shanzhai project in the current directory.",
  "version": "0.0.3",
  "engines": {
    "node": ">=12.13.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.*",
    "!test-data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/shanzhai"
  },
  "license": "MIT",
  "dependencies": {
    "@shanzhai/change-tracking-helpers": "0.0.7",
    "@shanzhai/execution-helpers": "0.0.0",
    "@shanzhai/planning-helpers": "0.0.3",
    "@shanzhai/plugin-helpers": "0.0.5"
  },
  "devDependencies": {
    "@shanzhai/create-directory-step": "0.0.2",
    "@shanzhai/interfaces": "0.0.16",
    "@shanzhai/read-text-file-step": "0.0.2",
    "@shanzhai/serial-step": "0.0.2",
    "@shanzhai/value-store": "0.0.4",
    "@shanzhai/write-file-step": "0.0.2",
    "@types/fs-extra": "9.0.12",
    "@types/uuid": "8.3.1",
    "fs-extra": "10.0.0",
    "uuid": "8.3.2"
  },
  "bin": {
    "shanzhai-production-cli": "."
  },
  "types": "index.d.ts"
}
