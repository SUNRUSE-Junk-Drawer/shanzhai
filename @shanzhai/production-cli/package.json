{
  "name": "@shanzhai/production-cli",
  "description": "A CLI tool which performs a one-off production build of the Shanzhai project in the current directory.",
  "version": "0.0.16",
  "engines": {
    "node": ">=17.6.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.*",
    "!test-data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/shanzhai"
  },
  "license": "MIT",
  "dependencies": {
    "@shanzhai/change-tracking-helpers": "0.0.12",
    "@shanzhai/execution-helpers": "0.0.8",
    "@shanzhai/plugin-helpers": "0.0.11",
    "@shanzhai/planning-helpers": "0.0.8"
  },
  "devDependencies": {
    "@shanzhai/create-directory-step": "0.0.9",
    "@shanzhai/ephemeral-unkeyed-store": "0.0.4",
    "@shanzhai/interfaces": "0.0.24",
    "@shanzhai/read-text-file-step": "0.0.10",
    "@shanzhai/serial-step": "0.0.10",
    "@shanzhai/unkeyed-store-get-input": "0.0.4",
    "@shanzhai/unkeyed-store-set-output": "0.0.4",
    "@shanzhai/write-file-step": "0.0.9",
    "@types/fs-extra": "9.0.13",
    "@types/uuid": "8.3.4",
    "fs-extra": "10.0.1",
    "uuid": "8.3.2"
  },
  "bin": "./index.js",
  "types": "index.d.ts"
}
