{
  "name": "@shanzhai/zip-plugin",
  "description": "A Shanzhai plugin which zips the generated content.",
  "version": "0.0.1",
  "engines": {
    "node": ">=12.13.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.*",
    "!test-data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/shanzhai"
  },
  "license": "MIT",
  "dependencies": {
    "@shanzhai/key-value-store": "0.0.8",
    "@shanzhai/serial-step": "0.0.5",
    "@shanzhai/value-store": "0.0.6",
    "@shanzhai/zip-step": "0.0.5"
  },
  "devDependencies": {
    "@shanzhai/interfaces": "0.0.16",
    "@shanzhai/zip-content-store": "0.0.0",
    "@shanzhai/zip-store": "0.0.0"
  },
  "peerDependencies": {
    "@shanzhai/zip-content-store": "0.0.0",
    "@shanzhai/zip-store": "0.0.0"
  },
  "types": "index.d.ts",
  "shanzhaiPlugin": [
    "."
  ]
}
