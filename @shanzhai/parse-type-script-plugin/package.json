  "devDependencies": {
    "@shanzhai/interfaces": "0.0.16",
    "@shanzhai/parsed-type-script-store": "0.0.5",
    "@shanzhai/type-script-compiler-options-store": "0.0.4",
    "@shanzhai/type-script-source-store": "0.0.5"
  },
  "peerDependencies": {
    "@shanzhai/type-script-source-store": "0.0.5",
    "@shanzhai/type-script-compiler-options-store": "0.0.4",
    "@shanzhai/parsed-type-script-store": "0.0.5"
