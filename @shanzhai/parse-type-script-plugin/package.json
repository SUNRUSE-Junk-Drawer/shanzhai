{
  "name": "@shanzhai/parse-type-script-plugin",
  "description": "A Shanzhai plugin which parses TypeScript source files.",
  "version": "0.0.0",
  "engines": {
    "node": ">=12.13.0"
  },
  "engineStrict": true,
