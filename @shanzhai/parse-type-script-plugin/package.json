  "devDependencies": {
    "@shanzhai/interfaces": "0.0.21",
    "@shanzhai/parsed-type-script-store": "0.0.9",
    "@shanzhai/type-script-compiler-options-store": "0.0.8",
    "@shanzhai/type-script-source-store": "0.0.8"
  },
  "peerDependencies": {
    "@shanzhai/type-script-source-store": "0.0.7",
    "@shanzhai/type-script-compiler-options-store": "0.0.7",
