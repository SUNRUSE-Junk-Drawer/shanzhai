{
  "name": "shanzhai",
  "version": "999.999.999",
  "engines": {
    "node": ">=12.13.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/shanzhai"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/html-minifier": "4.0.0",
    "@types/jasmine": "3.5.12",
    "@types/jsdom": "16.2.4",
    "@types/node": "13.13.15",
    "@types/pixelmatch": "5.2.2",
    "@types/progress": "2.0.3",
    "@types/pug": "2.0.4",
    "@types/sharp": "0.26.0",
    "@types/svgo": "1.3.3",
    "@types/uuid": "8.0.1",
    "@typescript-eslint/eslint-plugin": "4.1.1",
    "@typescript-eslint/parser": "4.1.1",
    "del-cli": "3.0.1",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "extract-zip": "2.0.1",
    "jasmine": "3.6.1",
    "jsdom": "16.4.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "pixelmatch": "5.2.1",
    "prettier": "2.0.5",
    "sharp": "0.26.0"
  },
  "scripts": {
    "buildClean": "del-cli \"**/*.js\" \"**/*.d.ts\" \"!**/node_modules/**/*\"",
    "buildTypeScript": "tsc",
    "buildJasmineUnit": "jasmine --config=unit.jasmine.json",
    "buildNyc": "nyc --check-coverage --all --exclude \"**/*.ts\" --exclude \"**/*.d.ts\" --exclude \"**/unit.*\" --lines 100 --functions 100 --branches 100 --statements 100 npm run-script buildJasmineUnit",
    "test": "npm-run-all buildClean buildTypeScript buildNyc",
    "lint": "eslint \"**/*.ts\""
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.js",
    "!**/unit.d.ts"
  ],
  "dependencies": {
    "7zip-bin": "5.0.3",
    "chokidar": "3.4.2",
    "html-minifier": "4.0.0",
    "jsonschema": "1.2.6",
    "pug": "3.0.0",
    "svgo": "1.3.2",
    "typescript": "4.0.3",
    "uglify-js": "3.10.4",
    "uuid": "8.3.0"
  }
}
